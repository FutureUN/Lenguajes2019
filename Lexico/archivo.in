



global sizes 
   # matrix dimension, default 10
   var N := 10 
   # number of processes, default 2
   var PR := 2 
   # strip size
   var S : int 
body sizes 
   getarg(1, N); getarg(2, PR); S := N/PR 


do  end  extend 
   if N mod PR != 0 -> 
     write("N must be a multiple of PR"); stop (1) 

global 
   fi 
   a5 mod  gaga 43 
end

